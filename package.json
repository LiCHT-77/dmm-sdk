{
  "name": "@licht-77/dmm-sdk",
  "version": "1.0.5",
  "description": "DMM Affiliate API v3 SDK for Node.js/TypeScript",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/cjs/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "jest",
    "build": "rm -rf dist && tsc -p tsconfig.json && tsc -p tsconfig.esm.json",
    "prepare": "npm run build"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LiCHT-77/dmm-sdk"
  },
  "keywords": [
    "dmm",
    "affiliate",
    "api",
    "sdk"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/LiCHT-77/dmm-sdk/issues"
  },
  "homepage": "https://github.com/LiCHT-77/dmm-sdk#readme",
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.14.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.3.2",
    "typescript": "^5.8.3"
  }
}
